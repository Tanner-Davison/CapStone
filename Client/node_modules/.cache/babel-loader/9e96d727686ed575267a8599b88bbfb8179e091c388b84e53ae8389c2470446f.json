{"ast":null,"code":"export const useIsTyping=(socket,username,room,typingTimeoutId)=>{const handleIsTyping=event=>{if(event.nativeEvent.inputType===\"deleteContentBackward\"){return;}clearTimeout(typingTimeoutId.current);typingTimeoutId.current=setTimeout(()=>{const data={username:username,room:room};socket.emit(\"typing\",data);},300);};return handleIsTyping;};","map":{"version":3,"names":["useIsTyping","socket","username","room","typingTimeoutId","handleIsTyping","event","nativeEvent","inputType","clearTimeout","current","setTimeout","data","emit"],"sources":["C:/Users/Tanner/Desktop/Chatty-Chatter/Chatty_Chatter/Client/src/components/Utility-mainRoom/IsTyping.js"],"sourcesContent":["\r\nexport const useIsTyping = (socket, username, room, typingTimeoutId) => {\r\n  const handleIsTyping = (event) => {\r\n    if (event.nativeEvent.inputType === \"deleteContentBackward\") {\r\n      return;\r\n    }\r\n\r\n    clearTimeout(typingTimeoutId.current);\r\n\r\n    typingTimeoutId.current = setTimeout(() => {\r\n      const data = {\r\n        username: username,\r\n        room: room,\r\n      };\r\n      socket.emit(\"typing\", data);\r\n    }, 300);\r\n  };\r\n\r\n  return handleIsTyping;\r\n};\r\n"],"mappings":"AACA,MAAO,MAAM,CAAAA,WAAW,CAAGA,CAACC,MAAM,CAAEC,QAAQ,CAAEC,IAAI,CAAEC,eAAe,GAAK,CACtE,KAAM,CAAAC,cAAc,CAAIC,KAAK,EAAK,CAChC,GAAIA,KAAK,CAACC,WAAW,CAACC,SAAS,GAAK,uBAAuB,CAAE,CAC3D,OACF,CAEAC,YAAY,CAACL,eAAe,CAACM,OAAO,CAAC,CAErCN,eAAe,CAACM,OAAO,CAAGC,UAAU,CAAC,IAAM,CACzC,KAAM,CAAAC,IAAI,CAAG,CACXV,QAAQ,CAAEA,QAAQ,CAClBC,IAAI,CAAEA,IACR,CAAC,CACDF,MAAM,CAACY,IAAI,CAAC,QAAQ,CAAED,IAAI,CAAC,CAC7B,CAAC,CAAE,GAAG,CAAC,CACT,CAAC,CAED,MAAO,CAAAP,cAAc,CACvB,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}